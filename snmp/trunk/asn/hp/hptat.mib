 hpTatAgtDriveDstPercent OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
		STATUS  mandatory
        DESCRIPTION
		    "The percentage of DST progress"
        ::= { hpTatAgtDriveEntry 14 }

 hpTatAgtDriveDstStart OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-write
		STATUS  mandatory
        DESCRIPTION
		    "Start or abort DST."
        ::= { hpTatAgtDriveEntry 15 }

 hpTatAgtDriveDstResults OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
		STATUS  mandatory
        DESCRIPTION
		    "DST result"
        ::= { hpTatAgtDriveEntry 16 }

hpTatAgtDriveDstCompletionTime OBJECT-TYPE
        SYNTAX  INTEGER
        ACCESS  read-only
		STATUS  mandatory
        DESCRIPTION
