        SYNTAX  INTEGER {
                        NotReady(0),
                        Ready(1)                }
        ACCESS  read-write
        STATUS  mandatory
        DESCRIPTION
                "Setting this object to Ready(1) will kick off the up/download process
                 specified in wbt3UpDnLoadTable"
        ::={wbt3UpDnLoad 4}


