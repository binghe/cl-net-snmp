IMPORTS 
        MODULE-IDENTITY, 
        OBJECT-TYPE,
        Unsigned32
                FROM SNMPv2-SMI
        MODULE-COMPLIANCE, 
        OBJECT-GROUP
                FROM SNMPv2-CONF
        DisplayString,
        RowStatus,
        TruthValue,
        StorageType,
        TEXTUAL-CONVENTION
                FROM SNMPv2-TC 
        InetAddressType,
        InetAddress,
        InetPortNumber
                FROM INET-ADDRESS-MIB
        SnmpAdminString
                FROM SNMP-FRAMEWORK-MIB
        ciscoMgmt
                FROM CISCO-SMI
--        cLWlanConfigEntry
--                FROM CISCO-LWAPP-WLAN-MIB;
        cLWlanIndex
                FROM CISCO-LWAPP-WLAN-MIB;


